{"ast":null,"code":"import firebase from \"firebase\";\nexport default {\n  data() {\n    return {\n      form: {\n        email: null,\n        password: null\n      },\n      emailIsValid: true,\n      passwordIsValid: true\n    };\n  },\n  methods: {\n    submitForm() {\n      const emailIsValid = this.validEmail(this.form.email);\n      const password = !!this.form.password;\n      const formIsValid = emailIsValid && password;\n      if (formIsValid) {\n        this.emailIsValid = true;\n        this.passwordIsValid = true;\n        console.log('Form sumitted', this.form);\n      } else {\n        this.emailIsValid = emailIsValid;\n        this.passwordIsValid = password;\n      }\n    },\n    validEmail: function (email) {\n      var re = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\n      return re.test(email);\n    },\n    login(e) {\n      firebase.auth().signInWithEmailAndPassword(this.email, this.password).then(user => {\n        console.log(user);\n      }, err => {\n        alert(err.message);\n      });\n      e.preventDefault();\n    },\n    loginWithProvider() {\n      var provider = new firebase.auth.GoogleAuthProvider();\n      firebase.auth().signInWithPopup(provider).then(user => {\n        console.log(user);\n      }, err => {\n        alert(err.message);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAwDA,OAAOA,QAAO,MAAO,UAAU;AAC/B,eAAe;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,IAAG,EAAI;QACHC,KAAK,EAAE,IAAG;QACVC,QAAQ,EAAE;MACd;MACAC,YAAW,EAAI,IAAG;MAClBC,eAAc,EAAI;IACtB;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,UAAU,GAAE;MAER,MAAMH,YAAW,GAAI,IAAI,CAACI,UAAU,CAAC,IAAI,CAACP,IAAI,CAACC,KAAK;MACpD,MAAMC,QAAO,GAAI,CAAC,CAAC,IAAI,CAACF,IAAI,CAACE,QAAO;MACpC,MAAMM,WAAU,GAAIL,YAAW,IAAKD,QAAO;MAE3C,IAAGM,WAAW,EAAC;QACX,IAAI,CAACL,YAAW,GAAI,IAAI;QACxB,IAAI,CAACC,eAAc,GAAI,IAAI;QAC3BK,OAAO,CAACC,GAAG,CAAC,eAAe,EAAC,IAAI,CAACV,IAAI;MAC