{"ast":null,"code":"import { ref } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'Login Page',\n  setup() {\n    const login_form = ref({});\n    const register_form = ref({});\n    const store = useStore();\n    const login = () => {\n      store.dispatch('login', login_form.value);\n    };\n    const register = () => {\n      store.dispatch('register', register_form.value);\n    };\n    return {\n      login_form,\n      register_form,\n      login,\n      register\n    };\n  },\n  data() {\n    return {\n      form: {\n        email: null,\n        password: null\n      },\n      emailIsValid: true,\n      passwordIsValid: true\n    };\n  },\n  methods: {\n    submitForm() {\n      const emailIsValid = this.validEmail(this.form.email);\n      const password = !!this.form.password;\n      const formIsValid = emailIsValid && password;\n      if (formIsValid) {\n        this.emailIsValid = true;\n        this.passwordIsValid = true;\n        console.log('Form sumitted', this.form);\n        this.loginWithEmail();\n      } else {\n        this.emailIsValid = emailIsValid;\n        this.passwordIsValid = password;\n      }\n    },\n    validEmail: function (email) {\n      var re = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\n      return re.test(email);\n    }\n  }\n};","map":{"version":3,"mappings":"AAyDA,SAASA,GAAE,QAAS,KAAI;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,eAAe;EACX;EACAC,IAAG,EAAI,YAAY;EACnBC,KAAI,GAAK;IACX,MAAMC,UAAS,GAAIJ,GAAG,CAAC,CAAC,CAAC,CAAC;IAC1B,MAAMK,aAAY,GAAIL,GAAG,CAAC,CAAC,CAAC,CAAC;IAC7B,MAAMM,KAAI,GAAIL,QAAQ,EAAE;IACxB,MAAMM,KAAI,GAAI,MAAM;MACnBD,KAAK,CAACE,QAAQ,CAAC,OAAO,EAAEJ,UAAU,CAACK,KAAK,CAAC;IAC1C;IACA,MAAMC,QAAO,GAAI,MAAM;MACtBJ,KAAK,CAACE,QAAQ,CAAC,UAAU,EAAEH,aAAa,CAACI,KAAK,CAAC;IAChD;IACA,OAAO;MACNL,UAAU;MACVC,aAAa;MACbE,KA