lex-row items-center mt-8\">\n        <h2 class=\"text-lg font-medium mr-auto\">All Course</h2>\n    </div>\n    <div class=\"grid grid-cols-12 mt-5\">\n        <!-- BEGIN: Inbox Side Menu -->\n        <div class=\"box col-span-12 2xl:col-span-9\">\n            <div class=\"grid grid-cols-12 gap-6\">\n                <div class=\"col-span-12 xl:col-span-4 2xl:col-span-3 min-w-[240px] border-b xl:border-r border-slate-200/60 bg-slate-50 p-5 rounded-l-md\">\n                    <div>\n                        <a\n                        href=\"\"\n                        class=\"flex items-center px-3 py-2 rounded-md bg-primary font-medium text-white\"\n                        >\n                            <MailIcon class=\"w-4 h-4 mr-2\" /> ALL\n                        </a>\n                        <a href=\"\" class=\"flex items-center px-3 py-2 mt-2 rounded-md\">\n                            <StarIcon class=\"w-4 h-4 mr-2\" /> ผู้เริ่มต้น\n                        </a>\n                        <a href=\"\" class=\"flex items-center px-3 py-2 mt-2 rounded-md\">\n                            <InboxIcon class=\"w-4 h-4 mr-2\" /> พื้นฐาน\n                        </a>\n                        <a href=\"\" class=\"flex items-center px-3 py-2 mt-2 rounded-md\">\n                            <SendIcon class=\"w-4 h-4 mr-2\" /> ขั้นกลาง\n                        </a>\n                        <a href=\"\" class=\"flex items-center px-3 py-2 mt-2 rounded-md\">\n                            <TrashIcon class=\"w-4 h-4 mr-2\" /> ขั้นสูง\n                        </a>\n                        <a href=\"\" class=\"flex items-center px-3 py-2 mt-2 rounded-md\">\n                            <StarIcon class=\"w-4 h-4 mr-2\" /> จิตวิทยา\n                        </a>\n                    </div>\n\n                    <div class=\"border-t border-slate-400/20 mt-4 pt-4\">\n                        <a href=\"\" class=\"flex items-center px-3 py-2 truncate\">\n                            <div class=\"w-2 h-2 bg-pending rounded-full mr-3\"></div>\n                            Free\n                        </a>\n                        <a href=\"\" class=\"flex items-center px-3 py-2 mt-2 rounded-md truncate\">\n                            <div class=\"w-2 h-2 bg-success rounded-full mr-3\"></div>\n                            Start\n                        </a>\n                        <a href=\"\" class=\"flex items-center px-3 py-2 mt-2 rounded-md truncate\">\n                            <div class=\"w-2 h-2 bg-warning rounded-full mr-3\"></div>\n                            Silver\n                        </a>\n                        <a href=\"\" class=\"flex items-center px-3 py-2 mt-2 rounded-md truncate\">\n                            <div class=\"w-2 h-2 bg-pending rounded-full mr-3\"></div>\n                            Gold\n                        </a>\n                        <a href=\"\" class=\"flex items-center px-3 py-2 mt-2 rounded-md truncate\">\n                            <div class=\"w-2 h-2 bg-danger rounded-full mr-3\"></div>\n                            VVIP\n                        </a>\n                    </div>\n                </div>\n                <!-- END: Inbox Side Menu -->\n                <!-- BEGIN: Inbox Content -->\n                <div class=\"inbox col-span-12 xl:col-span-8 2xl:col-span-9\">\n                    <div class=\"px-5 pt-10 pb-4 grid grid-cols-12 2xl:grid-cols-8 gap-3 sm:gap-6 border-b border-slate mb-5\" >\n                        <div\n                            v-for=\"(item, i) in file\"\n                            :key=\"i\"\n                            class=\"intro-y col-span-6 sm:col-span-4 md:col-span-3 2xl:col-span-2\"\n                        >\n\n                        <!-- file level -->\n                        <div v-if=\"userLevel < item.level\">\n                            <div class=\"another-level-file\" @click=\"ModalNoAccess = !ModalNoAccess\">\n                                <div class=\"another-level-text h-1/2\">\n                                    <span class=\"text-xl\">เฉพาะผู้สนับสนุน</span>\n                                    <span class=\"mt-3 text-xl\">LEVEL VVIP เท่านั้น</span>\n                                </div>\n                            </div>\n                            <div class=\"another-level-text mt-10\">\n                                <button \n                                @click=\"ModalAdvert = !ModalAdvert\"\n                                class=\"bg-white text-black px-4 py-1 rounded-full text-md hover:text-white hover:bg-primary\">\n                                    ตัวอย่าง\n                                </button>\n                            </div>\n                        </div>\n                        <!-- file level -->\n\n\n\n                            <div class=\"file box border-slate-200/60 shadow-none rounded-md px-5 pt-8 pb-5 px-3 sm:px-5 relative zoom-in\" >\n                                <div class=\"absolute left-0 top-0 mt-3 ml-3\">\n                                    <input\n                                        class=\"form-check-input\"\n                                        type=\"checkbox\"\n                                        :checked=\"item.trueFalse\"\n                                    />\n                                </div>\n                                <a\n                                    v-if=\"item.type == 'Empty Folder'\"\n                                    href=\"\"\n                                    class=\"w-3/5 file__icon file__icon--empty-directory mx-auto\"\n                                ></a>\n                                <a\n                                    v-else-if=\"item.type == 'Folder'\"\n                                    href=\"\"\n                                    class=\"w-3/5 file__icon file__icon--directory mx-auto\"\n                                >\n                                    <div class=\"file__icon--image__preview image-fit\">\n                                        <img\n                                        alt=\"Rocketman Tailwind HTML Admin Template\"\n                                        src=\"@/assets/images/folder.jpg\"\n                                        />\n                                    </div>\n                                \n                                </a>\n                                <a\n                                v-else-if=\"item.type == 'Image'\"\n                                href=\"\"\n                                class=\"w-3/5 file__icon file__icon--image mx-auto\"\n                                >\n                                    <div class=\"file__icon--image__preview image-fit\">\n                                        <img\n                                        alt=\"Rocketman Tailwind HTML Admin Template\"\n                                        src=\"@/assets/images/test-user.png\"\n                                        />\n                                    </div>\n                                </a>\n                                <a v-else href=\"\" class=\"w-3/5 file__icon file__icon--file mx-auto\">\n                                    <div class=\"file__icon__file-name\">\n                                        {{ item.type }}\n                                    </div>\n                                </a>\n                                <a href=\"\" class=\"block font-medium mt-4 text-center truncate\">\n                                {{ item.fileName }}</a>\n                                <div class=\"text-slate-500 text-xs text-center mt-0.5\">\n                                    {{ item.size }}\n                                </div>\n                                <Dropdown click outside class=\"absolute top-0 right-0 mr-2 mt-3 ml-auto\">\n                                    <DropdownToggle tag=\"a\" class=\"w-5 h-5 block\" href=\"javascript:;\">\n                                        <MoreVerticalIcon class=\"w-5 h-5 text-slate-500\" />\n                                    </DropdownToggle>\n                                    <DropdownMenu class=\"w-40\">\n                                        <DropdownContent>\n                                        <DropdownItem>\n                                            <UsersIcon class=\"w-4 h-4 mr-2\" /> Share File\n                                        </DropdownItem>\n                                        <DropdownItem>\n                                            <TrashIcon class=\"w-4 h-4 mr-2\" /> Delete\n                                        </DropdownItem>\n                                        </DropdownContent>\n                                    </DropdownMenu>\n                                </Dropdown>\n                            </div>\n                            </div>\n                    </div>\n\n                </div>\n            <!-- END: Inbox Content -->\n            </div>\n        </div>\n        <div class=\"col-span-12 2xl:col-span-3 pt-6 2xl:pt-0\">\n            <div class=\"2xl:border-l border-slate-300/50 h-full pb-6\">\n                <div class=\"2xl:pl-6 grid grid-cols-12 gap-x-6 gap-y-3\">\n                    <!-- BEGIN: Attachments -->\n                    <div v-if=\"!isFinishVideo\" class=\"box intro-x py-4 px-2 flex col-span-12 md:col-span-6 2xl:col-span-12\">\n                        <div class=\"w-[40%] flex justify-center items-center\">\n                            <div class=\"flex flex-col justify-center items-center text-center py-4\">\n                                <span class=\"text-xxl font-bold\" style=\"font-size:45px;\">90%</span>\n                                <span class=\"text-xxl font-bold mt-4\">LEVEL ปัจจุบัน</span>\n                            </div>\n                        </div>\n                        <div class=\"w-4/6 border-l border-slate-300/100 flex justify-center items-center text-center\">\n                                <div class=\"whitespace-nowrap text-sm\">\n                                    ยินดีด้วยคุณเป็นผู้สนับสนุนรายใหญ่<br>\n                                    ของ MONEYLECTURE แล้ว\n                                </div>\n                        </div> \n                    </div>\n\n                    <div v-if=\"isFinishVideo\" class=\"box intro-x py-4 px-2 flex col-span-12 md:col-span-6 2xl:col-span-12\">\n                        <div class=\"w-[40%] flex justify-center items-center\">\n                            <div class=\"flex flex-col justify-center items-center text-center py-4\">\n                                <span class=\"text-xxl font-bold\" style=\"font-size:45px;\">100%</span>\n                                <span class=\"text-xxl font-bold mt-4 whitespace-nowrap\">ความคืบหน้าการเรียน</span>\n                            </div>\n                        </div>\n                        <div class=\"w-4/6 border-l border-slate-300/100 flex justify-center items-center text-center\">\n                                <div class=\"flex flex-col\">\n                                    <button class=\"btn btn-primary rounded-full w-36 h-[50px]\">\n                                    รับ Certificate\n                                    </button>\n                                </div>\n                        </div> \n                    </div>\n                     <!-- END: Attachments -->\n                     <div class=\"box intro-x py-4 px-2 flex col-span-12 md:col-span-6 2xl:col-span-12\">\n                        <div \n                        @click=\"ModalImage = !ModalImage\"\n                        class=\"bg-slate-300 h-60 w-full cursor-pointer\">\n    \n                        </div>\n                    </div>\n                     <!-- BEGIN: Important Notes -->\n                    <div class=\"intro-x col-span-12 md:col-span-6 2xl:col-span-12\">\n                         <div class=\"mt-4\">\n                            <div class=\"box p-5\">\n                                <div class=\"pb-5 border-b border-dashed border-slate-200 flex justify-between items-center\">\n                                    <div class=\"text-xl font-bold\">ชื่อคอส...</div>\n                                    <EditIcon class=\"w-4 h-4 mr-2\" />\n                                </div>\n                                <div class=\"mt-5\">\n                                    <div class=\"intro-y flex items-center px-3 py-3\">\n                                        <div class=\"w-2 h-2 bg-pending rounded-full mr-3\"></div>\n                                        เนื้อหา : 7 บทเรียน\n                                    </div>\n                                    <div class=\"intro-y flex items-center px-3 py-3\">\n                                        <div class=\"w-2 h-2 bg-pending rounded-full mr-3\"></div>\n                                        ระยะเวลาเรียนได้ : ตามระดับ\n                                    </div>\n                                    <div class=\"intro-y flex items-center px-3 py-3\">\n                                        <div class=\"w-2 h-2 bg-pending rounded-full mr-3\"></div>\n                                        ชั่วโมงเรียน : 56 ชั่วโมง\n                                    </div>\n                                    <div class=\"intro-y flex items-center px-3 py-3\">\n                                        <div class=\"w-2 h-2 bg-pending rounded-full mr-3\"></div>\n                                        Certificate : มี\n                                    </div>\n                                </div>\n                            </div>\n                         </div>\n                    </div>\n\n                    <div class=\"intro-x col-span-12 md:col-span-6 2xl:col-span-12\">\n                         <div class=\"mt-4\">\n                            <div class=\"box p-5\">\n                                <div class=\"pb-5 border-b border-dashed border-s